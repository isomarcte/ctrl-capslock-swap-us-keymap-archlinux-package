#/bin/bash  -*- mode: shell-script; -*-

function post_install {
    echo -e '\nTo enable the keymap run reload the system manager configuration (or reboot) and use localctl to select the keymap.\n'
    echo -e '\t$ sudo systemctl daemon-reload'
    echo -e '\t$ sudo localectl set-keymap ctrl-capslock-swap-us\n'
}
